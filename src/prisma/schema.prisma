generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DB_URL")
}

model User {
  id                 Int       @id @default(autoincrement())
  name               String
  email              String    @unique
  phoneNumber        String?   @unique
  dateOfBirth        String?
  age                Int?
  gender             String?
  encrypted_password String
  role               String?   @default("USER")
  createdAt          DateTime? @default(now())
}
